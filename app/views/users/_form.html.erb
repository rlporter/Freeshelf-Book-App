
<h1>Sign Up</h1>
<%= form_for :user, url: '/users/new' do |f| %>
<p>
  <%= f.label :username %><br />
  <%= f.text_field :username %>
</p>

<p>
  <%= f.label :email %><br />
  <%= f.text_field :email%>
</p>

<p>
  <%= f.label :password %><br />
  <%= f.password_field :password_digest %>
</p>

<p>
  <%= f.label :passord_confirmation %><br />
  <%= f.password_field :password_confirmation %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>
